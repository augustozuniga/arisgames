<?xml version="1.0" ?>
<v:GameEditorContainerView xmlns:mx="http://www.adobe.com/2006/mxml"
                  xmlns:main="org.arisgames.editor.*"
                  xmlns:v="org.arisgames.editor.view.*"
                  xmlns:c="org.arisgames.editor.components.*"
                  width="300"
                  height="100%"
                  >

    <v:GameEditorObjectPaletteMX id="gameObjectPalette" width="300" height="100%"/>
    <v:GameEditorMapMX id="gameEditor" x="301" width="100%" height="100%"/>
    <v:GameEditorObjectEditorMX id="gameEditorObjectEditor" x="301" y="-275" width="100%" height="275"/>

    <mx:Move id="panelOut" target="{gameEditorObjectEditor}" yTo="0"  duration="1500"/>
    <mx:Move id="panelIn" target="{gameEditorObjectEditor}" yTo="-275" duration="1000"/>

</v:GameEditorContainerView>